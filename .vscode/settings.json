  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.workingDirectories": ["./ui", "./server"],
  "javascript.validate.enable": true,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    ".next": true,
    "**/*/lib": true,
    "**/*/dist": true,
    "**/*/build": true,
    "**/.webpack": true
  },
  "files.watcherExclude": {
    "**/*/lib": true,
    "**/*/coverage": true,
    "**/*/dist": true,
    "**/*/build": true,
    "**/.webpack": true
  },
  "search.exclude": {
    "**/*/lib": true,
    "**/*/coverage": true,
    "**/*/dist": true,
    "**/*/build": true,
    "**/.webpack": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
