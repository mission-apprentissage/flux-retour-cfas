{
  "name": "mna-flux-retour-cfas",
  "version": "2.55.0",
  "description": "[MNA] Flux retour des cfas",
  "repository": "https://github.com/mission-apprentissage/flux-retour-cfas.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "exports": "./src/index.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config src/index.js",
    "watch": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "cross-env DOTENV_CONFIG_PATH=.env.example mocha --config=mocharc.json tests/integration",
    "test:single": "cross-env DOTENV_CONFIG_PATH=.env.example mocha --config=mocharc.json",
    "lint": "eslint src/ tests/",
    "docker:start": "cd .. && docker-compose up -d --build --force-recreate",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes",
    "seed:cfas": "node -r dotenv/config src/jobs/seed/seedCfas.js",
    "seed:reseauxCfas": "node -r dotenv/config src/jobs/seed/seedReseauxCfas.js",
    "seed:sample": "node -r dotenv/config src/jobs/seed/seedSample.js",
    "seed:randomizedSample": "node -r dotenv/config src/jobs/seed/seedRandomizedSample.js",
    "seed:clearAssets": "node -r dotenv/config src/jobs/seed/clearAssets.js",
    "seed:formations-rncp": "node -r dotenv/config src/jobs/seed/seedFormationsRncps.js",
    "seed:referentiel-siret-uai": "node -r dotenv/config src/jobs/seed/seedReferentielSiretUai.js",
    "users:create": "node -r dotenv/config src/jobs/users/create/index.js",
    "users:create-erp-user": "node -r dotenv/config src/jobs/users/create-erp-user/index.js",
    "users:generate-password-update-token": "node -r dotenv/config src/jobs/users/generate-password-update-token/index.js",
    "clear:dossiersApprenants-networks": "node -r dotenv/config src/jobs/remove-data/clear/clearDossiersApprenantsNetworks.js",
    "remove:inscritsSansContrats-nonRecus-depuis": "node -r dotenv/config src/jobs/remove-data/statuts-inscritsSsContrats-nonRecus-depuis/index.js",
    "remove:apprentis-nonRecus-depuis": "node -r dotenv/config src/jobs/remove-data/statuts-apprentis-nonRecus-depuis/index.js",
    "remove:dossiersApprenants-nonRecus-depuis": "node -r dotenv/config src/jobs/remove-data/dossiers-apprenants-nonRecus-depuis/index.js",
    "purge:events": "node -r dotenv/config src/jobs/remove-data/purgeEvents/index.js",
    "seed:formations": "node -r dotenv/config src/jobs/seed/seedFormations.js",
    "create:effectifsApprenants-collection": "node -r dotenv/config src/jobs/create-effectifs-apprenants-collection/index.js",
    "dossiersApprenants:retrieve-networks": "node -r dotenv/config src/jobs/dossiersApprenants-retrieve-networks/index.js",
    "support:identify-network-duplicates": "node -r dotenv/config src/jobs/support/identify-network-duplicates/index.js",
    "support:remove-dossiersApprenants-duplicates": "node -r dotenv/config src/jobs/support/dossiersApprenants-duplicates/remove-duplicates.js",
    "support:identify-uais-duplicates": "node -r dotenv/config src/jobs/support/dossiersApprenants-duplicates-uais/identify.js",
    "support:identify-cfd-duplicates": "node -r dotenv/config src/jobs/support/dossiersApprenants-duplicates-cfd/identify.js",
    "support:clean-uais-duplicates": "node -r dotenv/config src/jobs/support/dossiersApprenants-duplicates-uais/clean.js",
    "retrieve-formation-rncp-in-tco": "node -r dotenv/config src/jobs/retrieve-formation-rncp-in-tco/index.js",
    "retrieve-location-from-uai": "node -r dotenv/config src/jobs/retrieve-location-from-uai/index.js",
    "retrieve-nature-organismes-de-formation": "node -r dotenv/config src/jobs/organisme-de-formation-retrieve-nature-in-referentiel/index.js",
    "cache:warmup": "node -r dotenv/config src/jobs/warm-effectifs-cache/index.js",
    "indexes:create": "node -r dotenv/config src/jobs/create-indexes/index.js",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down"
  },
  "dependencies": {
    "@faker-js/faker": "7.2.0",
    "arg": "5.0.1",
    "async": "3.2.3",
    "axios": "0.27.2",
    "body-parser": "1.20.0",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "cli-progress": "3.11.0",
    "convert-csv-to-json": "1.3.3",
    "cross-env": "7.0.3",
    "date-fns": "2.28.0",
    "env-var": "7.1.1",
    "express": "4.18.1",
    "fs-extra": "10.1.0",
    "iconv-lite": "0.6.3",
    "joi": "17.6.0",
    "json-stringify-deterministic": "1.0.6",
    "json2csv": "5.0.7",
    "jsonwebtoken": "8.5.1",
    "lodash.groupby": "4.6.0",
    "lodash.indexby": "3.1.1",
    "lodash.omit": "4.5.0",
    "lodash.omitby": "4.6.0",
    "lodash.pick": "4.4.0",
    "lodash.sortby": "4.7.0",
    "migrate-mongo": "9.0.0",
    "mongodb": "4.11.0",
    "node-ovh-objectstorage": "2.0.5",
    "oleoduc": "0.6.0",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "randexp": "0.5.3",
    "redis": "4.1.0",
    "sha512crypt-node": "1.0.2",
    "uuid": "8.3.2",
    "xlsx": "0.18.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "axiosist": "0.10.0",
    "dotenv": "16.0.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "mocha": "10.0.0",
    "mockdate": "3.0.5",
    "mongodb-memory-server": "8.5.2",
    "nock": "13.2.4",
    "nodemon": "2.0.16",
    "prettier": "2.6.2"
  }
}
