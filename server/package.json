{
  "name": "mna-flux-retour-cfas",
  "version": "2.27.0",
  "description": "[MNA] Flux retour des cfas",
  "repository": "https://github.com/mission-apprentissage/flux-retour-cfas.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "watch": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "mocha -r dotenv/config --recursive --exit --timeout 5000 tests/",
    "lint": "eslint src/ tests/",
    "docker:start": "docker-compose up -d --build --force-recreate",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes",
    "seed:users": "node -r dotenv/config src/jobs/seed/seedUsers.js",
    "seed:sample": "node -r dotenv/config src/jobs/seed/seedSample.js",
    "seed:randomizedSample": "node -r dotenv/config src/jobs/seed/seedRandomizedSample.js",
    "seed:referentielCfas": "node -r dotenv/config src/jobs/seed/seedReferentielCfas.js",
    "seed:annuaireCfas": "node -r dotenv/config src/jobs/seed/seedAnnuaireCfas.js",
    "clear:all": "node -r dotenv/config src/jobs/clear/clearAll.js",
    "clear:statutsCandidats": "node -r dotenv/config src/jobs/clear/clearStatutsCandidats.js",
    "clear:statutsCandidats-networks": "node -r dotenv/config src/jobs/clear/clearStatutsCandidatsNetworks.js",
    "clear:logs": "node -r dotenv/config src/jobs/clear/clearLogs.js",
    "clear:users": "node -r dotenv/config src/jobs/clear/clearUsers.js",
    "clear:cfas": "node -r dotenv/config src/jobs/clear/clearCfas.js",
    "stats": "node -r dotenv/config src/jobs/compute-stats-statuts-candidats/index.js",
    "run:tests": "node -r dotenv/config src/jobs/test/index.js",
    "siret:retrieve-gesti": "node -r dotenv/config src/jobs/siret/retrieve-sirets-from-gesti-uais.js",
    "siret:retrieve-ymag": "node -r dotenv/config src/jobs/siret/retrieve-sirets-from-ymag-uais.js",
    "siret:sanitize": "node -r dotenv/config src/jobs/siret/sanitize-sirets.js",
    "siret:check-validity": "node -r dotenv/config src/jobs/siret/check-siret-validity.js",
    "uai:check-validity": "node -r dotenv/config src/jobs/uai/check-uai-validity.js",
    "formation:retrieve-from-cfd": "node -r dotenv/config src/jobs/formation-retrieve-from-cfd/index.js",
    "statutsCandidats:retrieve-location": "node -r dotenv/config src/jobs/statutsCandidats-retrieve-location/index.js",
    "statutsCandidats:retrieve-networks": "node -r dotenv/config src/jobs/statutsCandidats-retrieve-networks/index.js",
    "statutsCandidats:retrieve-niveaux": "node -r dotenv/config src/jobs/statutsCandidats-retrieve-niveaux/index.js",
    "statutsCandidats:retrieve-sirets-catalogue": "node -r dotenv/config src/jobs/statutsCandidats-retrieve-sirets-catalogue/index.js",
    "statutsCandidats:retrieve-cfd-history": "node -r dotenv/config src/jobs/statutsCandidats-retrieve-cfd-history/index.js",
    "statutsCandidats:sanitize-empty-strings": "node -r dotenv/config src/jobs/statutsCandidats-sanitize-empty-strings/index.js",
    "cfas:retrieve-data-connection": "node -r dotenv/config src/jobs/cfas-retrieve-data-connection/index.js",
    "support:identify-cma": "node -r dotenv/config src/jobs/identify-cfas-network/identify-cma.js",
    "support:identify-network-duplicates": "node -r dotenv/config src/jobs/support/identify-network-duplicates/index.js",
    "support:identify-statutsCandidats-duplicates": "node -r dotenv/config src/jobs/support/statutsCandidats-duplicates/identify-duplicates.js",
    "support:remove-statutsCandidats-duplicates": "node -r dotenv/config src/jobs/support/statutsCandidats-duplicates/remove-duplicates.js",
    "support:identify-cfas-with-invalid-siret": "node -r dotenv/config src/jobs/support/identify-cfas-with-invalid-siret/index.js",
    "support:identify-uais-sirets-duplicates": "node -r dotenv/config src/jobs/support/identify-uais-sirets-duplicates/index.js",
    "support:identify-uais-catalog": "node -r dotenv/config src/jobs/support/identify-uais-catalog/index.js",
    "retrieve-location-from-uai": "node -r dotenv/config src/jobs/retrieve-location-from-uai/index.js",
    "fix-historique-date-statut-format": "node -r dotenv/config src/jobs/fix-historique-date-statut-format/index.js",
    "calculate:stats": "node -r dotenv/config src/jobs/stats/index.js",
    "indexes:create": "node -r dotenv/config src/jobs/create-indexes/index.js",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down"
  },
  "dependencies": {
    "@mission-apprentissage/tco-service-node": "2.18.1",
    "arg": "5.0.0",
    "async": "3.2.0",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.14",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "cli-progress": "3.9.0",
    "compose-middleware": "5.0.1",
    "csv-parse": "4.14.1",
    "date-fns": "2.21.3",
    "env-var": "7.0.0",
    "express": "4.17.1",
    "faker": "5.1.0",
    "fs-extra": "9.0.1",
    "iconv-lite": "0.6.2",
    "joi": "17.3.0",
    "json2csv": "5.0.5",
    "jsonwebtoken": "8.5.1",
    "lodash.groupby": "4.6.0",
    "lodash.indexby": "3.1.1",
    "lodash.omit": "4.5.0",
    "lodash.omitby": "4.6.0",
    "lodash.sortby": "4.7.0",
    "migrate-mongo": "8.1.4",
    "mongoose": "5.10.18",
    "mongoose-paginate": "5.0.3",
    "node-ovh-objectstorage": "2.0.1",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "randexp": "0.5.3",
    "requestretry": "4.1.2",
    "sha512crypt-node": "0.1.0",
    "tail": "2.0.4",
    "xlsx": "0.16.9"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "axiosist": "0.9.0",
    "dotenv": "8.2.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "mocha": "8.2.1",
    "nock": "13.0.7",
    "nodemon": "2.0.7",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/preventSensibleFilesCommit.sh && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
