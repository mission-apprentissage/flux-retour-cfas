{
  "name": "mna-flux-retour-cfas",
  "version": "3.2.0-beta.2",
  "description": "[MNA] Flux retour des cfas",
  "repository": "https://github.com/mission-apprentissage/flux-retour-cfas.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "exports": "./src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "cross-env DOTENV_CONFIG_PATH=.env.example c8 --check-coverage mocha --config=mocharc.json tests/integration",
    "test:single": "cross-env DOTENV_CONFIG_PATH=.env.example mocha --config=mocharc.json",
    "lint": "eslint src/ tests/",
    "docker:start": "cd .. && docker-compose up -d --build --force-recreate",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes",
    "cli": "node src/jobs/cli.js",
    "cli:migration": "node src/jobs/cli.migration.js",
    "cli:fiabilisation": "node src/jobs/cli.fiabilisation.js",
    "indexes:create": "node -r dotenv/config src/jobs/create-indexes/index.js",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down"
  },
  "dependencies": {
    "arg": "5.0.1",
    "async": "3.2.3",
    "axios": "0.27.2",
    "axios-retry": "3.3.1",
    "body-parser": "1.20.0",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "checksum-stream": "1.0.3",
    "clamscan": "2.1.2",
    "cli-progress": "3.11.0",
    "commander": "9.4.1",
    "compose-middleware": "5.0.1",
    "convert-csv-to-json": "1.3.3",
    "cookie-parser": "1.4.6",
    "date-fns": "2.28.0",
    "deepmerge": "4.2.2",
    "dotenv": "16.0.1",
    "ejs": "3.1.7",
    "enjoi": "9.0.1",
    "env-var": "7.1.1",
    "express": "4.18.1",
    "fs-extra": "10.1.0",
    "iconv-lite": "0.6.3",
    "joi": "17.6.0",
    "joi-password": "4.0.0",
    "json2csv": "5.0.7",
    "jsonwebtoken": "8.5.1",
    "lodash-es": "4.17.21",
    "lodash.groupby": "4.6.0",
    "lodash.indexby": "3.1.1",
    "lodash.omit": "4.5.0",
    "lodash.omitby": "4.6.0",
    "lodash.pick": "4.4.0",
    "lodash.sortby": "4.7.0",
    "luxon": "2.3.0",
    "migrate-mongo": "9.0.0",
    "mjml": "4.11.0",
    "mongodb": "4.11.0",
    "multiparty": "4.2.3",
    "node-ovh-objectstorage": "2.0.5",
    "nodemailer": "6.8.0",
    "nodemailer-html-to-text": "3.2.0",
    "oleoduc": "0.7.0",
    "omit-deep": "0.3.0",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "passport-localapikey": "0.0.3",
    "randexp": "0.5.3",
    "rate-limiter-flexible": "2.3.6",
    "redis": "4.1.0",
    "safe-stable-stringify": "2.4.1",
    "sha512crypt-node": "1.0.2",
    "tcp-port-used": "1.0.2",
    "uuid": "8.3.2",
    "xlsx": "0.18.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "c8": {
    "all": true,
    "lines": 40,
    "functions": 45,
    "branches": 70,
    "statements": 40,
    "exclude": [
      "tests/**",
      "migrations/**",
      ".history/**"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "axiosist": "0.10.0",
    "c8": "7.12.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "mocha": "10.0.0",
    "mockdate": "3.0.5",
    "mongodb-memory-server": "8.5.2",
    "nock": "13.2.4",
    "nodemon": "2.0.16",
    "prettier": "2.6.2"
  }
}
