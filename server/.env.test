# .env file example for tableau de bord de l'apprentissage server container

## APPLICATION & ENV VARIABLES
MNA_TDB_ENV=test
MNA_TDB_PUBLIC_URL=test
MNA_TDB_MONGODB_URI=mongodb://127.0.0.1:27017/TDB-test-{{JEST_WORKER_ID}}?retryWrites=false
MNA_TDB_BODY_PARSER_LIMIT=100kb

## LOG VARIABLES
MNA_TDB_LOG_LEVEL=fatal

## AUTH VARIABLES
MNA_TDB_AUTH_USER_JWT_SECRET=1234
MNA_TDB_AUTH_ACTIVATION_JWT_SECRET=1234
MNA_TDB_AUTH_PASSWORD_JWT_SECRET=1234

## DEFAULT USER VARIABLES
MNA_TDB_USERS_DEFAULT_ADMIN_NAME=mna-admin
MNA_TDB_USERS_DEFAULT_ADMIN_PASSWORD=test
MNA_TDB_USERS_DEFAULT_ADMIN_PERMISSIONS=administrator

## OVH-STORAGE USER VARIABLES
MNA_TDB_OVH_STORAGE_USERNAME=hello
MNA_TDB_OVH_STORAGE_PASSWORD=test
MNA_TDB_OVH_STORAGE_TENANT_ID=opj
MNA_TDB_OVH_STORAGE_CONTAINER_NAME=mna-flux
MNA_TDB_OVH_STORAGE_ENCRYPTION_KEY=randomSuperKey
MNA_TDB_OVH_STORAGE_URI=

## SMTP
MNA_TDB_SMTP_HOST=
MNA_TDB_SMTP_PORT=
MNA_TDB_SMTP_AUTH_USER=
MNA_TDB_SMTP_AUTH_PASS=

## API KEY
MNA_TDB_ORGANISMES_CONSULTATION_API_KEY="_MySuperApiKey!Protectâ‚¬D"
MNA_TDB_API_ENTREPRISE_KEY="__MySuperApiKeyEntreprise"
MNA_TDB_API_DECA_URL=https://deca-api
MNA_TDB_API_DECA_LOGIN=decaLogin
MNA_TDB_API_DECA_PASSWORD=decaPassword


## SENTRY
SENTRY_DSN="https://public@sentry.example.com/1"
